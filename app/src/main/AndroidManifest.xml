<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.MinesweeperDemo">

    <application
        android:allowBackup="true"
        android:icon="@drawable/dig_logo_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/dig_logo_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.Minesweeper_se_tang_hu">
        <activity android:name="com.example.MinesweeperDemo.results"></activity>
        <activity android:name="com.example.MinesweeperDemo.GamePlay" />
        <activity android:name="com.example.MinesweeperDemo.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>